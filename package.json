{
  "name": "personal-finance-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "deploy": "npm run build && mv functions functions_bak && wrangler pages deploy dist --project-name=wealthpath --commit-dirty=true && mv functions_bak functions",
